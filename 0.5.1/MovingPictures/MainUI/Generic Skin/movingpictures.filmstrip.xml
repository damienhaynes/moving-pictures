<!-- 
This file should contain all items visible when the plug-in
is on the browser screen in the filmstrip view mode. All controls 
should use the following tag for proper visibility 
    
<visible>facadeview.filmstrip+Control.IsVisible(50)</visible>     
-->


<window>
  <controls>
    <define>#filmstrip.available:true</define>

  
    <control>
      <description>View Info</description>
      <type>label</type>
      <id>0</id>
      <posX>920</posX>
      <posY>100</posY>
      <width>486</width>
      <font>font14</font>
      <label>facadeview.filmstrip</label>
      <align>left</align>
      <visible>facadeview.filmstrip+Control.IsVisible(50)</visible>
    </control>
    
  </controls>
</window>