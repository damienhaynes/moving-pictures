                    <Value>//css_reference "MpeCore.dll";

using MpeCore.Classes;
using MpeCore;
using System;
using System.IO;

public class Script
{
    public static void Main(PackageClass packageClass, ActionItem actionItem)
    {
        string dbPath = Path.Combine(MpeInstaller.TransformInRealPath("%Database%"), "movingpictures.db3");
        if (!File.Exists(dbPath)) return;
        string backupPath = Path.Combine(MpeInstaller.TransformInRealPath("%Database%"), "MovingPictures_Backup");
        backupPath = Path.Combine(backupPath, DateTime.Now.ToString("yyyyMMdd_HHmmss"));
        string backupFile = Path.Combine(backupPath, "movingpictures.db3");
        try
        {
          Directory.CreateDirectory(backupPath);
          File.Copy(dbPath, backupFile, false);
        }
        catch { }
        return;
    }
}</Value>
                    <ValueType>Script</ValueType>
